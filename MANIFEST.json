{
  "manifest_version": 3,
  "name": "Website Blocker",
  "description": "Block access to specified websites based on specfied time",
  "version": "1.0.0",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],
  "default_locale": "en",
  "background": {
    "scripts": ["blocker.js"],
    "persistent": false
  },
  "action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "24": "icons/icon-24.png",
      "32": "icons/icon-32.png"
    },
    "default_title": "Click Me",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "24": "icons/icon-24.png",
    "32": "icons/icon-32.png"
  },
  "author": "mz"
}
